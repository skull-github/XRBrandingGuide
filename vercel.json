{
  "version": 2,
  "name": "mlb-xr-branding-guide",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {},
  "env": {},
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
